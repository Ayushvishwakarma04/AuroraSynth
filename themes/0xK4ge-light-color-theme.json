  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#B3A1B3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF6EC7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control Keywords",
      "scope": [
        "keyword.control",
        "keyword.operator",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#D72670",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#20d020"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#CC99FF"
      }
    },
    {
      "name": "Built-in Constants",
      "scope": [
        "constant.language",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#CC99FF"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "#B8005A"
      }
    },
    {
      "name": "Function Names",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#87CEEB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class Names",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#CC99FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "Attributes/Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property"
      ],
      "settings": {
        "foreground": "#FDFE9F"
      }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#FF6EC7"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "CSS Values",
      "scope": [
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#20d020"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FF6EC7"
      }
    },
    {
      "name": "Markdown Headers",
      "scope": [
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#FF6EC7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Code",
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code"
      ],
      "settings": {
        "foreground": "#20d020"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#CC99FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#B8005A"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#FF6EC7"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.section.brackets",
        "punctuation.section.parens",
        "punctuation.section.braces"
      ],
      "settings": {
        "foreground": "#B8005A"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#FF6B6B"
      }
    },
    {
      "name": "Invalid/Deprecated",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FF6B6B",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticTokenColors": {
    "variable": "#B8005A",
    "variable.readonly": "#FDFE9F",
    "parameter": "#B8005A",
    "function": "#87CEEB",
    "method": "#87CEEB",
    "class": "#CC99FF",
    "interface": "#87CEEB",
    "type": "#87CEEB",
    "enum": "#CC99FF",
    "enumMember": "#CC99FF",
    "namespace": "#FF6EC7",
    "keyword": "#FF6EC7",
    "string": "#20d020",
    "number": "#CC99FF",
    "regexp": "#6BCA8F",
    "operator": "#FF6EC7",
    "decorator": "#FF6EC7",
    "comment": "#B3A1B3"
  }
}